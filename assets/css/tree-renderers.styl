/***********************************************************************/
/* VARIABLES
/***********************************************************************/

$small-size = 14px;
$small-lineHieght = 20px;

$medium-size = 16px;
$medium-lineHieght = 24px;


/***********************************************************************/
/* MIXINS / FUNCTIONS
/***********************************************************************/

// taken from the nib.styl library
overflow()
  if arguments[0] == ellipsis
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
  else
    overflow: arguments

clearfix()
  &:before
  &:after
    content: ""
    display: table
  &:after
    clear: both


