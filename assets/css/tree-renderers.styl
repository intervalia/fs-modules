/*
 * PERSON INFORMATION
 *  <div class="person-info-container">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *      <li class="person-name">John Adam Doe</li>
 *      <li class="person-lifeSpan">1830-1900</li>
 *      <li class="person-id">SKJ-DIY</li>
 *    </ul>
 *  </div>
 *
 * COUPLE RELATIONSHIPS
 *  <div class="person-info-container couple-husband">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *     <li class="person-name">John Adam Doe</li>
 *      <li class="person-lifeSpan">1830-1900</li>
 *      <li class="person-id">SKJ-DIY</li>
 *    </ul>
 *  </div>
 *  <div class="person-info-container couple-wife">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *      <li class="person-name">John Adam Doe</li>
 *      <li class="person-lifeSpan">1830-1900</li>
 *      <li class="person-id">SKJ-DIY</li>
 *    </ul>
 *  </div>
 *
 * PEDIGREE RELATIONSHIPS
 *  <div class="person-info-container">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *      <li class="person-name">John Adam Doe</li>
 *      <li class="person-lifeSpan">1830-1900</li>
 *      <li class="person-id">SKJ-DIY</li>
 *    </ul>
 *  </div>
 *  <div class="person-info-container">
 *    <div class="person-gender-icon fs-icon-*"></div>
 *    <ul class="person-info">
 *      <li class="person-name add-spouse">Add Spouse</li>
 *    </ul>
 *  </div>
 */
.person-info-container {
  display: table;
  width: 100%;
  table-layout: fixed;

  // couple relationship
  &.couple-husband .person-gender-container,
  &.couple-wife .person-gender-container {
    width: 5px;
  }

  .person-gender-container {
    display: table-cell;
    width: 0;

    &.small {
      width: 15px;
    }

    &.medium {
      vertical-align: middle;
      width: 30px;
    }

    &.large {
      vertical-align: middle;
      width: 100px;
    }
  }

  .person-gender-icon {

    &[class*=fs-icon-] {
      display: inline-block;
      vertical-align: middle;
      position: relative;
    }

    &[class*=fs-icon-small] {
      vertical-align: top;
      margin-top: 7px;
    }
  }

  .person-info {
    padding: 0;
    margin: 0;
    list-style: none;
    display: table-cell;
    vertical-align: middle;

    clearfix();
  }

  .person-name {
    overflow: ellipsis;
    font-size: $medium-size;
    line-height: $medium-lineHieght;

    & > h1,& > h2,& > h3,& > h4 {
      padding: 0;
      margin: 0;
    }
  }
  .person-lifeSpan,
  .person-id,
  .person-separator {
    float: left;
    font-size: $small-size;
    line-height: $small-lineHieght;
  }

  // couple relationships
  &.couple-husband .person-gender-icon,
  &.couple-wife .person-gender-icon {
    @extend .person-info-container .person-gender-icon[class*=fs-icon-];

    &:before {
      content: '';
      width: 8px;
      height: 26px;
      border: 1px solid #000;
      border-right: 0;
      position: absolute;
      right: 100%;
    }
  }


  &.couple-husband .person-gender-icon:before {
    border-bottom: 0;
    top: 50%;
  }

  &.couple-wife {
    margin-top: 5px;

    .person-gender-icon:before {
      border-top: 0;
      bottom: 50%;
    }
  }

  // pedigree relationship
  .add-spouse {
    line-height: 40px;
  }
}





.person-vitals__name {
  overflow: ellipsis;
  font-size: $medium-size;
  line-height: $medium-lineHieght;
}

.person-vitals__name--split {
  display: none;
}

.person-vitals__vitals-container {
  clearfix();
}

.person-details__lifeSpan,
.person-details__separator,
.person-details__id,
.person-details__selection-stopper {
  float: left;
  font-size: $small-size;
  line-height: $small-lineHieght;
}

.person-vitals__name-full,
.person-vitals__name-given,
.person-vitals__name-family {
  overflow: ellipsis;
}

.person-info {
  display: table;
  width: 100%;
  table-layout: fixed;

  &__person-info {
    display: table-cell;
    vertical-align: middle;
  }

  .person-vitals {
    display: table-cell;
    vertical-align: middle;
  }

  // INLINE
  &--inline {

    .person-vitals__name {
      display: table-cell;
      vertical-align: top;
    }

    .person-vitals__person-details {
      display: table-cell;
      vertical-align: middle;
    }

    .person-vitals__vitals-container {
      margin-left: 30px;
    }

    .person-info__person-portrait {
      display: none;
    }
  }

  // STACKED
  &--stacked {

    .person-info__person-portrait,
    .person-info__person-info {
      display: table-row;
      text-align: center;
    }

    .person-info__person-gender {
      display: none;
    }

    .person-vitals__person-details {
      left: 50%;
      position: relative;
      float: left;
    }

    .person-vitals__vitals-container {
      left: -50%;
      position: relative;
      float: left;
    }

    .person-vitals__name:not(.person-vitals__name--sinotypic) {

      .person-vitals__name--split {
        display: inline;
      }

      .person-vitals__name-full {
        display: none;
      }
    }
  }
}

.person-info__person-portrait {
  display: table-cell;
  width: 85px;
}

.person-portrait__image {
  overflow: hidden;
  background-color: #e0e0e0;
  background-position: center;
  background-size: 60%;
  border: solid 4px #fff;
  border-radius: 50%;
  box-shadow: 0 2px 3px 0 #636363;
  display: inline-block;
  height: 66px;
  position: relative;
  text-align: center;
  width: 66px;
  z-index: 10;
}

.person-info__person-gender {
  display: table-cell;
  width: 0;

  &.small {
    width: 15px;
  }

  &.medium {
    vertical-align: middle;
    width: 30px;
  }

  &.large {
    vertical-align: middle;
    width: 100px;
  }
}

.person-gender__image {
  vertical-align: middle;
  display: inline-block;
  position: relative;

  &[class*=fs-icon-small] {
    vertical-align: top;
    margin-top: 7px;
  }
}